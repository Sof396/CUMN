<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.masInfo">

    <TextView
        android:id="@+id/masInfoTitulo"
        android:layout_width="329dp"
        android:layout_height="54dp"
        android:text="Titulo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/masInfoDescripcion"
        android:layout_width="327dp"
        android:layout_height="151dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/masInfoTitulo"
        app:layout_constraintVertical_bias="0.133" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapita"
        android:layout_width="331dp"
        android:layout_height="266dp"
        android:apiKey="AIzaSyAx28OEv8IwAP8anR2-fca9u_SOtAKQTig"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/masInfoDescripcion"
        app:layout_constraintVertical_bias="0.735" />

    <TextView
        android:id="@+id/masInfoOrganizacion"
        android:layout_width="331dp"
        android:layout_height="47dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/mapita"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/masInfoDescripcion" />


</androidx.constraintlayout.widget.ConstraintLayout>